{
  "name": "@diamondbot/urban-command",
  "version": "0.0.0",
  "description": "DiamondBot command that looks up word definitions from Urban Dictionary",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "bump:patch": "npm version patch --no-git-tag-version",
    "bump:minor": "npm version minor --no-git-tag-version",
    "bump:major": "npm version major --no-git-tag-version",
    "postversion": "npm publish --access public",
    "prepare": "npm run build",
    "build": "babel src -d dist",
    "build:watch": "babel src -d dist --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alvarocastro/diamondbot.git"
  },
  "keywords": [
    "diamondbot",
    "diamondbot command",
    "discord",
    "command",
    "urban",
    "dictionary",
    "urbandictionary",
    "definition",
    "slang",
    "random"
  ],
  "author": "Alvaro Castro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alvarocastro/diamondbot/issues"
  },
  "homepage": "https://github.com/alvarocastro/diamondbot/commands/urbandictionary#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.12.13"
  },
  "dependencies": {
    "@diamondbot/core": "^0.1.2"
  }
}
